<?xml version="1.0" encoding="UTF-8"?>
<web-app version="5.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee                               https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd">
    <display-name>Redes</display-name>
    
    <!-- Configuración del Servlet -->
    
    <servlet>
        <servlet-name>UsuarioServlet</servlet-name>
        <servlet-class>controlador.UsuarioServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UsuarioServlet</servlet-name>
        <url-pattern>/UsuarioServlet</url-pattern>
    </servlet-mapping>
    <servlet>
    
    <servlet-name>PacienteServlet</servlet-name>
        <servlet-class>controlador.PacienteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PacienteServlet</servlet-name>
        <url-pattern>/PacienteServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AseguradoServlet</servlet-name>
        <servlet-class>controlador.AseguradoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AseguradoServlet</servlet-name>
        <url-pattern>/AseguradoServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ExportarAseguradosServlet</servlet-name>
        <servlet-class>controlador.ExportarAseguradosServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ExportarAseguradosServlet</servlet-name>
        <url-pattern>/ExportarAseguradosServlet</url-pattern>
    </servlet-mapping>
                                            
    <!-- Página de inicio -->
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
